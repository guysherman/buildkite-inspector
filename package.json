{
  "name": "buildkite-inspector",
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc --project ./tsconfig.json",
    "lint": "eslint --ext .ts --ext .tsx src --max-warnings=0",
    "typecheck": "tsc --project ./tsconfig.json --noemit",
    "test": "jest --testPathPattern=src/ --runInBand",
    "test-watch": "jest --testPathPattern=src/ --runInBand --watch",
    "debug": "node --inspect-brk -r ts-node/register src/index.tsx",
    "dev": "ts-node src/index.tsx"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^7.28.0",
    "eslint-config-standard": "latest",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.0.4",
    "pinst": "^2.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@types/blessed": "^0.1.19",
    "@yarnpkg/pnpify": "^3.0.0-rc.6",
    "blessed": "^0.1.81",
    "node-fetch": "^3.0.0",
    "treecat": "link:../treecat"
  }
}
